<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rentbd">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.PostDetailsActivity"
            android:label="@string/post_details_activity"/>
        <activity android:name=".Activity.PostActivity"
            android:label="@string/post_activity"/>
        <activity android:name=".Activity.ToLetActivity"
            android:label="@string/to_let_activity"/>
        <activity android:name=".Activity.ResetPasswordActivity"
            android:label="@string/reset_password_activity"/>
        <activity android:name=".Activity.ProfileActivity"
            android:label="@string/profile_activity"/>
        <activity android:name=".Activity.SignUpActivity"
            android:label="@string/sign_up_activity"/>
        <activity
            android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.DashBordActivity"
            android:label="@string/dashbord_activity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>